<package version="0.0">
  <name>org.oxt.avaudiorecorder.paulmcclernan</name>
  <title>AVAudioRecorder library</title>
  <author>Paul McClernan</author>
  <version>0.1</version>
  <license>community</license>
  <type>library</type>
  <requires name="com.livecode.objc"/>
  <requires name="com.livecode.foreign"/>
  <requires name="com.livecode.engine"/>
  <requires name="com.livecode.arithmetic"/>
  <requires name="com.livecode.binary"/>
  <requires name="com.livecode.list"/>
  <requires name="com.livecode.logic"/>
  <requires name="com.livecode.string"/>
  <requires name="com.livecode.type"/>
  <requires name="com.livecode.system"/>
  <handler name="AVAudioRecorderStopCallback" parameters="in Pointer" return="any"/>
  <handler name="AVAudioRecorderInitCallback" parameters="" return="any"/>
  <handler name="AVAudioRecorderRemoveCallback" parameters="" return="any"/>
  <handler name="AVAudioRecorderLoad" parameters="in string" return="nothing"/>
  <handler name="AVAudioRecorderStop" parameters="" return="nothing"/>
  <handler name="AVAudioRecorderPlay" parameters="" return="any"/>
  <handler name="AVAudioRecorderIsPlaying" parameters="" return="boolean"/>
  <handler name="AVAudioRecorderSetRate" parameters="in real" return="nothing"/>
  <handler name="AVAudioRecorderDuration" parameters="" return="string"/>
  <handler name="AVAudioRecorderGetCurrentPosition" parameters="" return="string"/>
  <handler name="AVAudioRecorderSetCurrentPosition" parameters="in real" return="nothing"/>
</package>